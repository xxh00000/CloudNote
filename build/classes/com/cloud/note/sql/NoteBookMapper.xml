<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace命名空间,作用就是对sql进行分类化管理(sql隔离) -->
<mapper namespace="com.cloud.note.dao.NoteBookDao">
   <select id="findByUserId" parameterType="String" resultType="com.cloud.note.entity.NoteBook">
   select * from cn_notebook where cn_user_id=#{cn_user_id} order by cn_notebook_createtime desc
   
   </select>
   <insert id="InsertNoteBook" parameterType="com.cloud.note.entity.NoteBook">
   insert into cn_notebook (cn_notebook_id,cn_user_id,cn_notebook_type_id,cn_notebook_name,cn_notebook_desc,cn_notebook_createtime)
   values
  ( #{cn_notebook_id},#{cn_user_id},#{cn_notebook_type_id},#{cn_notebook_name},#{cn_notebook_desc},#{cn_notebook_createtime})
   </insert>
</mapper>